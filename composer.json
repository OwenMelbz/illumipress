{
    "name": "owenmelbz/illumipress",
    "description": "Simple wrapper for the illuminate support, http response, request and validation classes for WordPress",
    "type": "library",
    "require": {
        "illuminate/http": "5.*",
        "illuminate/validation": "5.*",
        "kitetail/zttp": "^0.3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Owen",
            "email": "owenmelbz@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ekandreas/bladerunner",
                "version": "1.7.4",
                "dist": {
                    "url": "https://github.com/ekandreas/bladerunner/archive/1.7.4.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "autoload": {
        "files": [
            "./src/Validator.php",
            "./src/Response.php",
            "./src/Request.php",
            "./src/Blade.php",
            "./src/helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    }
}
